
.tp-box{
	width: 100%;
	min-height: 100vh;
	background: #FFFFFF;
}
.tp-panel{
}
.tp-panel>view{
	border-bottom: 1rpx solid #E1E1E1;
}
.tp-panel>view>input{
	color: #666666;
	font-size: 30rpx;
}
.tp-tips{
	font-size: 26rpx;
	line-height: 44rpx;
	color: #999999;
}
.tp-btn{
	width: 100%;
	height: 86rpx;
	line-height: 86rpx;
	background: #343436;
	box-shadow: 0rpx 8rpx 31rpx 1rpx rgba(147, 147, 147, 0.35);
	border-radius: 20rpx;
	font-size: 30rpx;
	font-weight: bold;
	color: #FFFFFF;
}
.vc-btn-disabled{
	background: #888888;
}
.tp-plc{
	font-size: 30rpx;
	font-weight: 500;
	color: #999999;
}

